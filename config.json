{
  "name": "Import Images",
  "type": "app",
  "categories": [
    "import",
    "images",
    "essentials"
  ],
  "description": "Drag and drop images to Supervisely, supported formats: .jpg, .jpeg, .mpo, .bmp, .png, .tiff, .tif, .webp, .nrrd",
  "docker_image": "supervisely/base-py-sdk:6.38.0",
  "main_script": "src/main.py",
  "modal_template": "src/modal.html",
  "modal_template_state": {
    "files": "",
    "normalize_exif": false,
    "remove_alpha_channel": false,
    "convert_tiff": false,
    "remove_source": true,
    "project_name": ""
  },
  "task_location": "workspace_tasks",
  "icon": "https://github.com/supervisely-ecosystem/import-images/releases/download/v1.0.0/icon.png",
  "icon_cover": true,
  "icon_background": "#FFFFFF",
  "instance_version": "6.5.1",
  "headless": true,
  "context_menu": {
    "context_category": "Import",
    "target": [
      "files_folder",
      "images_project",
      "images_dataset"
    ]
  },
  "poster": "https://github.com/supervisely-ecosystem/import-images/releases/download/v1.0.0/poster.png"
}